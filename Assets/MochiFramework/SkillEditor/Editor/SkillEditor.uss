#TrackMenuView {
    background-color: rgb(88, 88, 88);
}

#TrackMenuView:hover {
    background-color: rgb(107, 107, 107);
}

:focus {
    border-left-color: rgba(47, 191, 255, 255);
    border-right-color: rgba(47, 191, 255, 255);
    border-top-color: rgba(47, 191, 255, 255);
    border-bottom-color: rgba(47, 191, 255, 255);
    border-top-width: 2px;
    border-right-width: 2px;
    border-bottom-width: 2px;
    border-left-width: 2px;
}
